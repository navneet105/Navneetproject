package LEC24;

public class Student {
	private String name="Kaju";
	private int age=21;
	
	public Student(String name,int age) {
		this.name=name;
		this.age=age;
	}
//incapsulation 
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

}
