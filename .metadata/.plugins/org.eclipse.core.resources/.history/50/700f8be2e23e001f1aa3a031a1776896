package Lec4;

import java.util.Scanner;

public class Reverse_Num {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int rem;
		int dig=0;
		while(n!=0) {//n>0
			rem=n%10;
			dig=10*dig+rem;
			n=n/10;
			
		}
		System.out.println(dig);

	}

}
