package Lec2;

public class Pattern90 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 8;
        int rows = 1;
        while (rows <= n) {
            int j = 1;
            while (j <= n - rows) {
                System.out.print(1);
                j++;
            }
            j = 1;
            while (j <= rows) {
                System.out.print(rows);
                j++;
            }
            System.out.println();
            rows++;
        }
	}

}
