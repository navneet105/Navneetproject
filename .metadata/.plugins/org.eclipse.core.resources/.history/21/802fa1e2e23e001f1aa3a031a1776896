package Lec7;

public class Array_Swap_2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int arr[]= {10,20,5,6,7,8};
		System.out.println(arr[0]+" "+arr[1]);
		Swap(arr,0,1);
		System.out.println(arr[0]+" "+arr[1]);//array ke gher m gush ke chnage kr rha hu  hep mempry ke under index ke under change kr rh h tabhi ye dikhega 
//reference ke under m jake change krna pdega tabhi call bi value ho ta h
		
	}
	public static void Swap(int []arr,int i,int j) {
		int temp=arr[i];
		arr[i]=arr[j];
		arr[j]=temp;
	}

}
