package test;

public class feb {

	public static void main(String[] args) {
		int n=5;
		System.out.println(isPrime(0));

	}
	public static boolean isPrime(int n) {
		return isPri(n,2);
	}
	
	public static boolean isPri(int n,int i) {
		if(i*i>n) {
			return true;
		}
		else if(n%i==0) {
			return false;
		}else {
			return isPri(n,i+1);
		}
	}

}
