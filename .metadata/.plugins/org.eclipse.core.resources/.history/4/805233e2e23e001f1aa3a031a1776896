package Lec13Srting;

public class String_Demo_3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Hello"+10+20+"Bye");
		System.out.println("Hello"+(10+20)+"Bye");
		System.out.println(10+20+"Hello"+"Bye");
		String s="Codingblock";
		System.out.println(s.length());// string class ka method h(fxn)
		int []ar=new int[5];
		System.out.println(ar.length);//it is variable
		System.out.println(s.charAt(4));//indexing from  0
		System.out.println(s.substring(3));//3rd index se pura
		System.out.println(s.substring(2,9));

	}

}
